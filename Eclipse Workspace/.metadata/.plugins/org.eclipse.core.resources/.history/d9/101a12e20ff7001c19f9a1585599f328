{
	public static void main(String Wilfred [])
	{
		String name="wil";
		int id=7155;
		float percent=27.27f;
		char gender='M';
		System.out.println("Name      :  "+name);
		System.out.println("id        :  "+id);
		System.out.println("Percent   :  "+percent);
		System.out.println("Gender    :  "+gender);
	}
}